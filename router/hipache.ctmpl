{{range ls "service/router"}}
{{.Value}}
{{else}}
{
    "server": { {{range ls "service/router"}}
        "{{.Key}}": "{{.Value}}"{{else}}
        "accessLog": "/data/router/access_log",
        "accessLogMode": "file",
        "port": 8080,
        "workers": 5,
        "maxSockets": 100,
        "deadBackendTTL": 30,
        "tcpTimeout": 30,
        "httpKeepAlive": true{{end}}
    },
    "redisHost": "127.0.0.1",
    "redisPort": 6379,
    "redisMasterHost": "{{range service "redis"}}{{.Address}}{{end}}",
    "redisMasterPort": {{range service "redis"}}{{.Port}}{{end}}
}
{{end}}
